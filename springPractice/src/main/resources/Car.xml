<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
            http://www.springframework.org/schema/beans/spring-beans.xsd
            http://www.springframework.org/schema/context
            http://www.springframework.org/schema/context/spring-context-3.0.xsd">

	<bean id="myCar" class="org.jsp.springPractice.Car"> </bean>
	
	<bean id="user" class="org.jsp.springPractice.User">
		
		<property name="userName" value="ABC"></property>
		<property name="password" value="abc123"></property>
	</bean>
	
	<bean id="dataSource" class="org.jsp.springPractice.MyDataSource">
		
		<property name="driverClassName" value="Chotu-driver"></property>
		<property name="user" value="Abhishek"></property>
		<property name="password" value="abhi#45"></property>
		<property name="url" value="www.abhi.com"></property>
	</bean>
</beans>